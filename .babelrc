{
    "presets": ["next/babel"],
    "plugins": [
      ["transform-imports", {
        "material-ui/core": {
          "transform": "material-ui/core/${member}",
          "preventFullImport": true
        },
        "hurumap-ui/core": {
          "transform": "hurumap-ui/core/${member}",
          "preventFullImport": true
        }
      }],
      [
        "module-resolver",
        {
          "root": ["./src"],
          "alias": {
            "assets": "./src/assets",
            "components": "./src/components",
            "data": "./src/data",
            "lib": "./src/lib"
          }
        }
      ]
    ]
  }
